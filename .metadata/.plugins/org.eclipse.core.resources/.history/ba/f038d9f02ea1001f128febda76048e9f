package Exercice1;

import java.util.ArrayList;

public class Test {

	public double distance(Point p1, Point p2) {
		double distance = 0.;
		distance = Math.sqrt(Math.pow((p2.getX() - p1.getX()), 2) + Math.pow((p2.getY() - p1.getY()), 2));
		return distance;
	}
