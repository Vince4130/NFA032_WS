package Exercice2;

public class SigneProduit {

	public String signe(Integer a, Integer b) {
		String signe = "";
		
		String signeA = a.toString();
		String signeB = b.toString();
		
		if(signeA.contains("-")) {
			if(!signeB.contains("-")) {
				signe = "n√©gatif";
			}
		}
		
		return signe;
	}
	
	public static void main(String[] args) {
		
		SigneProduit signeP = new SigneProduit();
		
		Integer a = -1;
		Integer b = -5;
		
		System.out.println(signeP.signe(a, b));
		
		
	}

}
