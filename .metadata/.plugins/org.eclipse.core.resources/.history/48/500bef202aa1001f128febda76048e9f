package Exercice1;

public class Point {
	
	private int x;
	private int y;
	
	public Point () {
		this.x = 0.0d;
		this.y = 0.0d;
	}
	
	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}
	

}
