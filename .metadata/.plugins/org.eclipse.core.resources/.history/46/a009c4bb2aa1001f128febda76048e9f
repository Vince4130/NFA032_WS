package Exercice1;

public class Podouble {
	
	private double x;
	private double y;
	
	public Podouble () {
		this.x = 0.0d;
		this.y = 0.0d;
	}
	
	public Podouble(double x, double y) {
		this.x = x;
		this.y = y;
	}

	public double getX() {
		return x;
	}

	public void setX(double x) {
		this.x = x;
	}

	public double getY() {
		return y;
	}

	public void setY(double y) {
		this.y = y;
	}
	

}
